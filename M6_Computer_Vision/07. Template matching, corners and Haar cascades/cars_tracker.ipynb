   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "cap= cv2.VideoCapture('videos/video.avi')\n",
    "\n",
    "\n",
    "while True:\n",
    "    ret, frame = cap.read()\n",
    "\n",
    "    cv2.imshow(\"Frame\", frame)\n",
    "\n",
    "    key=cv2.waitKey(30)\n",
    "\n",
    "    if key == 27:\n",
    "        break\n",
    "\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()\n"
   ]
